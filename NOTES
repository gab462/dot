# systemctl enable cpupower
# add user to realtime group
# setup aurutils local repo

test vaapi: mpv --hwdec=auto [vid]
temporary install: pacman -S --asdeps [pkg]
list packages: pacman -Qe
list orphan packages: pacman -Qdtq

aurutils setup: https://gist.github.com/geosharma/afe1ea9ebe58cb67aaaba62a0d47bc7a
install aur package: aur sync [pkg] && pacman -S [pkg]
update aur packages: aur sync -u
update git aur packages: aur sync -u --rebuild [pkg]
